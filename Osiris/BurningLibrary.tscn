code = "shader_type canvas_item;

uniform sampler2D noise_tex;
uniform sampler2D gradient_tex;

uniform vec4 brighter_color : hint_color = vec4(1.0, 0.8, 0.0, 1.0);
uniform vec4 middle_color : hint_color  = vec4(1.0, 0.56, 0.0, 1.0);
uniform vec4 darker_color : hint_color  = vec4(0.64, 0.2, 0.05, 1.0);

uniform float spread : hint_range(0.0, 1.0) = 0.5;
uniform float pixelation : hint_range(0.0, 100.0) = 0.0;

void fragment()
{
    // Step 1: Compute pixelated UV
    vec2 pixel_uv = UV;
    if (pixelation > 0.0) {
        vec2 pixel_size = vec2(pixelation / 200.0); // Adjusted pixel size for better control
        pixel_uv = floor(UV / pixel_size) * pixel_size;
    }

    // Step 2: Compute animated noise using unmodified UV
    float noise_value = texture(noise_tex, UV + vec2(0.0, TIME * 0.1)).x;
    
    // Step 3: Sample gradient texture using pixelated UV
    float gradient_value = texture(gradient_tex, pixel_uv.yx).x;
    
    // Step 4: Apply procedural blending
    gradient_value -= smoothstep(spread, spread + 0.5, length(pixel_uv + vec2(-0.5, -0.5)) / spread);
    
    float step1 = step(noise_value, gradient_value);
    float step2 = step(noise_value, gradient_value - 0.2);
    float step3 = step(noise_value, gradient_value - 0.4);
    
    vec3 bd_color = mix(brighter_color.rgb, darker_color.rgb, step1 - step2);
    
    vec4 color = vec4(bd_color, step1);
    color.rgb = mix(color.rgb, middle_color.rgb, step2 - step3);
    
    COLOR = color;
}
"

